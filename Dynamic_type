void main() {

  
/* In Dart, when a variable is declared as a dynamic type,
   it can store any value, such as int and float.

The value of a dynamic variable can change over time
within the program.

*/
  
  
dynamic a = 88 ; 
  
print(a) ; 
  
a = "hello" ; 
 
print(a) ; 
  
a= "Dart";  
  
print(a) ; 
  
a= 10.4;
 
print(a);
  
a = true ; 
  
print(a);
}
